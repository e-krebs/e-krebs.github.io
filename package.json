  },
  "homepage": "https://github.com/e-krebs/e-krebs.github.io#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.16.3",
    "@types/node": "^20.19.26",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.22",
    "gh-pages": "^6.3.0",
    "parcel": "^2.16.3",
    "postcss": "^8.5.6",
    "process": "^0.11.10",
    "rimraf": "^5.0.10",
